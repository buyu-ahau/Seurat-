#!/usr/bin/env Rscript
# -*- coding: utf-8 -*-
# çŒªåŸºå› åˆ°äººç±»åŒæºåŸºå› è½¬æ¢å®Œæ•´è„šæœ¬
# ä½œè€…: buyu-ahau
# æ—¥æœŸ: 2025-07-11
# åŠ è½½å¿…è¦çš„åº“
suppressPackageStartupMessages({
    library(Seurat)
    library(biomaRt)
    library(qs)
    library(dplyr)
    library(SeuratDisk)
})
# è®¾ç½®è¾“å‡ºç›®å½•
output_dir <- "/disk192/users_dir/buyu/2025-7-7é‡æ–°æ³¨é‡Šç»“æœ/pySCENIC"
if(!dir.exists(output_dir)) {
    dir.create(output_dir, recursive = TRUE)
}
# ====================== æ­¥éª¤1: è¯»å–çŒªæ•°æ® ======================
print("========== æ­¥éª¤1: è¯»å–çŒªæ•°æ® ==========")
print("è¯»å–çŒªçš„Seuratå¯¹è±¡...")
seurat_obj <- qread("/disk192/users_dir/buyu/2025-7-7é‡æ–°æ³¨é‡Šç»“æœ/object_final_annotated_corrected_v2.qs")
print("åŸå§‹çŒªæ•°æ®ä¿¡æ¯:")
print(seurat_obj)
print(paste("åŸå§‹åŸºå› æ•°:", nrow(seurat_obj)))
print(paste("ç»†èƒæ•°:", ncol(seurat_obj)))
# è·å–çŒªåŸºå› ç¬¦å·
pig_genes <- rownames(seurat_obj)
print(paste("çŒªåŸºå› æ€»æ•°:", length(pig_genes)))
# ====================== æ­¥éª¤2: è¿æ¥æ•°æ®åº“ ======================
print("\n========== æ­¥éª¤2: è¿æ¥Ensemblæ•°æ®åº“ ==========")
tryCatch({
    # è¿æ¥çŒªåŸºå› æ•°æ®åº“
    pig_mart <- useMart("ensembl", dataset = "sscrofa_gene_ensembl")
    print("âœ… æˆåŠŸè¿æ¥çŒªåŸºå› æ•°æ®åº“")
    
    # è¿æ¥äººç±»åŸºå› æ•°æ®åº“
    human_mart <- useMart("ensembl", dataset = "hsapiens_gene_ensembl")
    print("âœ… æˆåŠŸè¿æ¥äººç±»åŸºå› æ•°æ®åº“")
}, error = function(e) {
    print("âŒ è¿æ¥æ•°æ®åº“å¤±è´¥ï¼Œè¯·æ£€æŸ¥ç½‘ç»œ")
    print(e)
    stop()
})
# ====================== æ­¥éª¤3: åŒæºåŸºå› æŸ¥è¯¢ ======================
print("\n========== æ­¥éª¤3: åŒæºåŸºå› æŸ¥è¯¢ ==========")
print("é€šè¿‡çŒªåŸºå› ç¬¦å·æŸ¥æ‰¾äººç±»åŒæºåŸºå› ...")
tryCatch({
    # è·å–çŒªåŸºå› çš„äººç±»åŒæºåŸºå› ä¿¡æ¯
    pig_to_human <- getBM(
        attributes = c("external_gene_name", "hsapiens_homolog_ensembl_gene", 
                      "hsapiens_homolog_associated_gene_name", "hsapiens_homolog_orthology_type"),
        filters = "external_gene_name",
        values = pig_genes,
        mart = pig_mart
    )
    print("âœ… çŒªåˆ°äººç±»åŒæºåŸºå› æŸ¥è¯¢å®Œæˆ")
    
    # æŸ¥çœ‹ç»“æœ
    print(paste("æŸ¥è¯¢ç»“æœè¡Œæ•°:", nrow(pig_to_human)))
    print("å‰10è¡Œç»“æœ:")
    print(head(pig_to_human, 10))
    
}, error = function(e) {
    print("âŒ åŒæºåŸºå› æŸ¥è¯¢å¤±è´¥")
    print(e)
    stop()
})
# ====================== æ­¥éª¤4: æ•°æ®æ¸…ç† ======================
print("\n========== æ­¥éª¤4: æ•°æ®æ¸…ç† ==========")
print("è¿‡æ»¤å’Œæ¸…ç†åŒæºåŸºå› æ•°æ®...")

# ç§»é™¤ç©ºçš„äººç±»åŸºå› ç¬¦å·
pig_to_human_filtered <- pig_to_human[
    !is.na(pig_to_human$hsapiens_homolog_associated_gene_name) & 
    pig_to_human$hsapiens_homolog_associated_gene_name != "", 
]
print(paste("è¿‡æ»¤åçš„åŒæºåŸºå› å¯¹æ•°:", nrow(pig_to_human_filtered)))
# ç§»é™¤é‡å¤çš„çŒªåŸºå› ï¼ˆä¿ç•™ç¬¬ä¸€ä¸ªåŒ¹é…ï¼‰
pig_to_human_unique <- pig_to_human_filtered[!duplicated(pig_to_human_filtered$external_gene_name), ]
print(paste("å»é‡åçš„åŒæºåŸºå› å¯¹æ•°:", nrow(pig_to_human_unique)))
# åˆ›å»ºåŸºå› æ˜ å°„
gene_mapping <- setNames(
    pig_to_human_unique$hsapiens_homolog_associated_gene_name, 
    pig_to_human_unique$external_gene_name
)

print("åŒæºåŸºå› è½¬æ¢ç»Ÿè®¡:")
print(paste("åŸå§‹çŒªåŸºå› æ•°:", length(pig_genes)))
print(paste("æ‰¾åˆ°åŒæºåŸºå› çš„æ•°é‡:", length(gene_mapping)))
print(paste("è½¬æ¢ç‡:", round(length(gene_mapping)/length(pig_genes)*100, 2), "%"))
# ====================== æ­¥éª¤5: åˆ›å»ºè½¬æ¢åçš„å¯¹è±¡ ======================
print("\n========== æ­¥éª¤5: åˆ›å»ºè½¬æ¢åçš„Seuratå¯¹è±¡ ==========")
# ç­›é€‰Seuratå¯¹è±¡ä¸­å­˜åœ¨åŒæºåŸºå› çš„æ•°æ®
genes_to_keep <- intersect(pig_genes, names(gene_mapping))
print(paste("ä¿ç•™çš„åŸºå› æ•°:", length(genes_to_keep)))
# å­é›†åŒ–Seuratå¯¹è±¡
print("è¿‡æ»¤Seuratå¯¹è±¡...")
seurat_obj_filtered <- subset(seurat_obj, features = genes_to_keep)
print("è¿‡æ»¤åçš„Seuratå¯¹è±¡:")
print(seurat_obj_filtered)
# è·å–æ–°çš„åŸºå› åç§°
new_gene_names <- gene_mapping[genes_to_keep]
# æ£€æŸ¥é‡å¤çš„äººç±»åŸºå› åç§°
print("æ£€æŸ¥é‡å¤çš„äººç±»åŸºå› åç§°...")
duplicated_table <- table(duplicated(new_gene_names))
print(duplicated_table)

if(duplicated_table["TRUE"] > 0) {
    duplicated_genes <- new_gene_names[duplicated(new_gene_names)]
    print(paste("é‡å¤çš„åŸºå› æ•°:", length(duplicated_genes)))
    print("å‰10ä¸ªé‡å¤åŸºå› :")
    print(head(duplicated_genes, 10))
}

# ä¸ºé‡å¤åŸºå› æ·»åŠ åç¼€
print("ä¸ºé‡å¤åŸºå› æ·»åŠ åç¼€...")
new_gene_names_unique <- make.unique(new_gene_names)

# åˆ›å»ºæ–°çš„assayæ•°æ®
new_counts <- seurat_obj_filtered@assays$RNA@layers$counts
new_data <- seurat_obj_filtered@assays$RNA@layers$data

# è®¾ç½®æ–°çš„åŸºå› åç§°
rownames(new_counts) <- new_gene_names_unique
rownames(new_data) <- new_gene_names_unique

# åˆ›å»ºæ–°çš„Seuratå¯¹è±¡
print("åˆ›å»ºæ–°çš„Seuratå¯¹è±¡...")
seurat_obj_human <- CreateSeuratObject(
    counts = new_counts,
    meta.data = seurat_obj_filtered@meta.data,
    project = "pig_to_human_homolog"
)
# æ·»åŠ æ ‡å‡†åŒ–æ•°æ®
seurat_obj_human@assays$RNA@layers$data <- new_data
# å¤åˆ¶å…¶ä»–ä¿¡æ¯
if(length(seurat_obj_filtered@reductions) > 0) {
    seurat_obj_human@reductions <- seurat_obj_filtered@reductions
}

print("æœ€ç»ˆè½¬æ¢ç»“æœ:")
print(seurat_obj_human)
print(paste("æœ€ç»ˆåŸºå› æ•°:", nrow(seurat_obj_human)))
print(paste("ç»†èƒæ•°:", ncol(seurat_obj_human)))
# æ£€æŸ¥ç»†èƒç±»å‹ä¿¡æ¯
if("celltype" %in% colnames(seurat_obj_human@meta.data)) {
    print("ç»†èƒç±»å‹åˆ†å¸ƒ:")
    print(table(seurat_obj_human@meta.data$celltype))
}

# ====================== æ­¥éª¤6: ä¿å­˜ç»“æœ ======================
print("\n========== æ­¥éª¤6: ä¿å­˜ç»“æœ ==========")
# ä¿å­˜è½¬æ¢åçš„å¯¹è±¡
qsave(seurat_obj_human, file.path(output_dir, "seurat_obj_pig_to_human_homolog.qs"))
print("âœ… è½¬æ¢åçš„Seuratå¯¹è±¡å·²ä¿å­˜")
# ä¿å­˜åŸºå› æ˜ å°„è¡¨
write.csv(pig_to_human_unique, file.path(output_dir, "pig_to_human_gene_mapping.csv"), row.names = FALSE)
print("âœ… åŸºå› æ˜ å°„è¡¨å·²ä¿å­˜")
# ä¿å­˜å»é‡åçš„åŸºå› æ˜ å°„è¡¨
gene_mapping_unique <- data.frame(
    pig_gene = names(gene_mapping), 
    human_gene = new_gene_names_unique,
    original_human_gene = gene_mapping,
    stringsAsFactors = FALSE
)
write.csv(gene_mapping_unique, file.path(output_dir, "gene_mapping_unique.csv"), row.names = FALSE)
print("âœ… å»é‡åŸºå› æ˜ å°„è¡¨å·²ä¿å­˜")
# ====================== æ­¥éª¤7: ç”ŸæˆLOOMæ–‡ä»¶ ======================
print("\n========== æ­¥éª¤7: ç”ŸæˆLOOMæ–‡ä»¶ ==========")

tryCatch({
    # ç”ŸæˆLOOMæ–‡ä»¶
    as.loom(seurat_obj_human, 
            filename = file.path(output_dir, "scenic_input_pig_to_human.loom"), 
            verbose = TRUE, 
            overwrite = TRUE)
    
    print("âœ… LOOMæ–‡ä»¶å·²ç”Ÿæˆ")
    
    # æ£€æŸ¥æ–‡ä»¶å¤§å°
    file_size <- file.info(file.path(output_dir, "scenic_input_pig_to_human.loom"))$size
    print(paste("æ–‡ä»¶å¤§å°:", round(file_size/1024/1024, 2), "MB"))
    
}, error = function(e) {
    print("âŒ LOOMæ–‡ä»¶ç”Ÿæˆå¤±è´¥:")
    print(e)
})
# ====================== æœ€ç»ˆæ€»ç»“ ======================
print("\n" %s% paste(rep("=", 60), collapse=""))
print("ğŸ‰ çŒªåŸºå› åˆ°äººç±»åŒæºåŸºå› è½¬æ¢å®Œæˆï¼")
print(paste(rep("=", 60), collapse=""))
print("æœ€ç»ˆç»Ÿè®¡:")
print(paste("åŸå§‹çŒªåŸºå› æ•°:", length(pig_genes)))
print(paste("æ‰¾åˆ°åŒæºåŸºå› æ•°:", length(gene_mapping)))
print(paste("æœ€ç»ˆä¿ç•™åŸºå› æ•°:", nrow(seurat_obj_human)))
print(paste("è½¬æ¢ç‡:", round(nrow(seurat_obj_human)/length(pig_genes)*100, 2), "%"))
print("\nç”Ÿæˆçš„æ–‡ä»¶:")
print(paste("- Seuratå¯¹è±¡:", file.path(output_dir, "seurat_obj_pig_to_human_homolog.qs")))
print(paste("- åŸºå› æ˜ å°„è¡¨:", file.path(output_dir, "pig_to_human_gene_mapping.csv")))
