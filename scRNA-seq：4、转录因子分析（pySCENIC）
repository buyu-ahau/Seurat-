####pyscenicåˆ†æ
#!/usr/bin/env python3
"""
pySCENIC Pipeline Script
Author: buyu-ahau
Date: 2025-07-11
Description: è‡ªåŠ¨åŒ–è¿è¡ŒpySCENICä¸‰æ­¥åˆ†ææµç¨‹
"""

import os
import sys
import subprocess
import time
import logging
from datetime import datetime

# è®¾ç½®æ—¥å¿—
def setup_logging(log_file):
    """è®¾ç½®æ—¥å¿—è®°å½•"""
    logging.basicConfig(
        level=logging.INFO,
        format='%(asctime)s - %(levelname)s - %(message)s',
        handlers=[
            logging.FileHandler(log_file),
            logging.StreamHandler(sys.stdout)
        ]
    )
    return logging.getLogger(__name__)

def run_command(cmd, logger, step_name):
    """è¿è¡Œshellå‘½ä»¤å¹¶è®°å½•è¾“å‡º"""
    logger.info(f"å¼€å§‹æ‰§è¡Œ: {step_name}")
    logger.info(f"å‘½ä»¤: {cmd}")
    
    start_time = time.time()
    
    try:
        # è¿è¡Œå‘½ä»¤
        result = subprocess.run(
            cmd, 
            shell=True, 
            capture_output=True, 
            text=True, 
            check=True
        )
        
        end_time = time.time()
        duration = end_time - start_time
        
        logger.info(f"{step_name} å®Œæˆ!")
        logger.info(f"è€—æ—¶: {duration:.2f} ç§’ ({duration/60:.2f} åˆ†é’Ÿ)")
        
        if result.stdout:
            logger.info(f"æ ‡å‡†è¾“å‡º: {result.stdout}")
        
        return True
        
    except subprocess.CalledProcessError as e:
        end_time = time.time()
        duration = end_time - start_time
        
        logger.error(f"{step_name} å¤±è´¥!")
        logger.error(f"é”™è¯¯ä»£ç : {e.returncode}")
        logger.error(f"é”™è¯¯ä¿¡æ¯: {e.stderr}")
        logger.error(f"å¤±è´¥å‰è€—æ—¶: {duration:.2f} ç§’")
        
        return False

def main():
    """ä¸»å‡½æ•°"""
    # è·¯å¾„è®¾ç½®
    BASE_DIR = "/disk192/users_dir/buyu/2025-7-7é‡æ–°æ³¨é‡Šç»“æœ/pySCENIC"
    INPUT_LOOM = f"{BASE_DIR}/input_data/scenic_input_raw_counts.loom"
    REF_DIR = f"{BASE_DIR}/å‚è€ƒæ–‡ä»¶"
    OUTPUT_DIR = f"{BASE_DIR}/output"
    LOG_FILE = f"{OUTPUT_DIR}/pyscenic_pipeline.log"
    
    # åˆ›å»ºè¾“å‡ºç›®å½•
    os.makedirs(OUTPUT_DIR, exist_ok=True)
    
    # è®¾ç½®æ—¥å¿—
    logger = setup_logging(LOG_FILE)
    
    logger.info("="*60)
    logger.info("pySCENIC Pipeline å¼€å§‹è¿è¡Œ")
    logger.info(f"å¼€å§‹æ—¶é—´: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}")
    logger.info("="*60)
    
    # æ£€æŸ¥è¾“å…¥æ–‡ä»¶
    logger.info("æ£€æŸ¥è¾“å…¥æ–‡ä»¶...")
    required_files = [
        INPUT_LOOM,
        f"{REF_DIR}/allTFs_hg38.txt",
        f"{REF_DIR}/hg19-tss-centered-10kb-10species.mc9nr.genes_vs_motifs.rankings.feather",
        f"{REF_DIR}/motifs-v10nr_clust-nr.hgnc-m0.001-o0.0.tbl"
    ]
    
    missing_files = []
    for file_path in required_files:
        if not os.path.exists(file_path):
            missing_files.append(file_path)
    
    if missing_files:
        logger.error("ä»¥ä¸‹å¿…éœ€æ–‡ä»¶ç¼ºå¤±:")
        for file_path in missing_files:
            logger.error(f"  - {file_path}")
        sys.exit(1)
    else:
        logger.info("æ‰€æœ‰å¿…éœ€æ–‡ä»¶éƒ½å­˜åœ¨")
    
    # æ­¥éª¤1: GRN (åŸºå› è°ƒæ§ç½‘ç»œæ¨æ–­)
    logger.info("\n" + "="*50)
    logger.info("æ­¥éª¤ 1/3: GRN (åŸºå› è°ƒæ§ç½‘ç»œæ¨æ–­)")
    logger.info("="*50)
    
    grn_cmd = f"""pyscenic grn \
  --num_workers 48 \
  --output {OUTPUT_DIR}/grn_output.tsv \
  --method grnboost2 \
  {INPUT_LOOM} \
  {REF_DIR}/allTFs_hg38.txt"""
    
    if not run_command(grn_cmd, logger, "GRNæ­¥éª¤"):
        logger.error("GRNæ­¥éª¤å¤±è´¥ï¼Œå°è¯•ä½¿ç”¨å¤‡ç”¨æ–¹æ³•...")
        
        # å¤‡ç”¨æ–¹æ³• - ä½¿ç”¨arboreto_with_multiprocessing.py
        backup_cmd = f"""python /home/buyu/miniconda3/envs/pyscenics/bin/arboreto_with_multiprocessing.py \
  --num_workers 48 \
  --output {OUTPUT_DIR}/grn_output.tsv \
  --method grnboost2 \
  {INPUT_LOOM} \
  {REF_DIR}/allTFs_hg38.txt"""
        
        if not run_command(backup_cmd, logger, "GRNæ­¥éª¤(å¤‡ç”¨æ–¹æ³•)"):
            logger.error("GRNæ­¥éª¤å®Œå…¨å¤±è´¥ï¼Œç»ˆæ­¢æµç¨‹")
            sys.exit(1)
    
    # æ­¥éª¤2: CTX (é¡ºå¼è°ƒæ§æ¨¡å—é¢„æµ‹)
    logger.info("\n" + "="*50)
    logger.info("æ­¥éª¤ 2/3: CTX (é¡ºå¼è°ƒæ§æ¨¡å—é¢„æµ‹)")
    logger.info("="*50)
    
    ctx_cmd = f"""pyscenic ctx \
  {OUTPUT_DIR}/grn_output.tsv \
  {REF_DIR}/hg19-tss-centered-10kb-10species.mc9nr.genes_vs_motifs.rankings.feather \
  --annotations_fname {REF_DIR}/motifs-v10nr_clust-nr.hgnc-m0.001-o0.0.tbl \
  --expression_mtx_fname {INPUT_LOOM} \
  --mode "dask_multiprocessing" \
  --output {OUTPUT_DIR}/ctx.csv \
  --num_workers 56 \
  --mask_dropouts"""
    
    if not run_command(ctx_cmd, logger, "CTXæ­¥éª¤"):
        logger.error("CTXæ­¥éª¤å¤±è´¥ï¼Œç»ˆæ­¢æµç¨‹")
        sys.exit(1)
    
    # æ­¥éª¤3: AUCell (è°ƒæ§å­æ´»æ€§è®¡ç®—)
    logger.info("\n" + "="*50)
    logger.info("æ­¥éª¤ 3/3: AUCell (è°ƒæ§å­æ´»æ€§è®¡ç®—)")
    logger.info("="*50)
    
    aucell_cmd = f"""pyscenic aucell \
  {INPUT_LOOM} \
  {OUTPUT_DIR}/ctx.csv \
  --output {OUTPUT_DIR}/aucell.loom \
  --num_workers 32"""
    
    if not run_command(aucell_cmd, logger, "AUCellæ­¥éª¤"):
        logger.error("AUCellæ­¥éª¤å¤±è´¥ï¼Œç»ˆæ­¢æµç¨‹")
        sys.exit(1)
    
    # å®Œæˆ
    logger.info("\n" + "="*60)
    logger.info("ğŸ‰ pySCENIC Pipeline å…¨éƒ¨å®Œæˆ! ğŸ‰")
    logger.info(f"å®Œæˆæ—¶é—´: {datetime.now().strftime('%Y-%m-%d %H:%M:%S')}")
    logger.info("="*60)
    
    logger.info("è¾“å‡ºæ–‡ä»¶:")
    logger.info(f"  - GRNç»“æœ: {OUTPUT_DIR}/grn_output.tsv")
    logger.info(f"  - CTXç»“æœ: {OUTPUT_DIR}/ctx.csv")
    logger.info(f"  - AUCellç»“æœ: {OUTPUT_DIR}/aucell.loom")
    logger.info(f"  - æ—¥å¿—æ–‡ä»¶: {LOG_FILE}")

if __name__ == "__main__":
    main()
